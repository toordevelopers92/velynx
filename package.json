{
  "name": "velynx-monorepo",
  "version": "0.2.7",
  "private": true,
  "description": "Framework",
  "keywords": [
    "velynx"
  ],
  "homepage": "https://github.com/toordevelopers92/velynx#readme",
  "bugs": {
    "url": "https://github.com/toordevelopers92/velynx/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/toordevelopers92/velynx.git"
  },
  "license": "ISC",
  "author": "Toor Developers Inc",
  "type": "commonjs",
  "main": "index.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "build": "pnpm -C packages/core build && pnpm -C packages/js build",
    "docs:prepare": "node scripts/prepare-docs.js",
    "docs:build": "pnpm docs:prepare && pnpm -C packages/docs docs:build",
    "docs:dev": "pnpm docs:prepare && pnpm -C packages/docs docs:dev",
    "test": "pnpm -C packages/core test && pnpm -C packages/js test",
    "lint": "pnpm -C packages/core lint && pnpm -C packages/js lint && pnpm -C packages/docs lint",
    "format": "prettier -w ."
  },
  "dependencies": {
    "eslint-scope": "^7.2.2",
    "eslint-visitor-keys": "^3.4.3"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^8.57.0",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "prettier": "^3.3.3",
    "stylelint": "^16.8.2",
    "stylelint-config-recommended": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.5.4"
  },
  "packageManager": "pnpm@9.0.0",
  "pnpm": {
    "overrides": {
      "esbuild": "0.21.5"
    }
  }
}





