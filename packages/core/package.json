{
  "name": "@toordevelopersinc/velynx",
  "version": "0.3.0",
  "description": "Velynx core CSS framework",
  "repository": "https://github.com/toordevelopers92/velynx",
  "homepage": "https://github.com/toordevelopers92/velynx#readme",
  "bugs": {
    "url": "https://github.com/toordevelopers92/velynx/issues"
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "dist/velynx.extended.css",
  "style": "dist/velynx.extended.css",
  "unpkg": "dist/velynx.extended.min.css",
  "jsdelivr": "dist/velynx.extended.min.css",
  "exports": {
    ".": {
      "default": "./dist/velynx.extended.css"
    },
    "./core": "./dist/velynx.core.css",
    "./tokens": "./dist/velynx.tokens.css",
    "./extended.min": "./dist/velynx.extended.min.css",
    "./core.min": "./dist/velynx.core.min.css",
    "./tokens.min": "./dist/velynx.tokens.min.css"
  },
  "scripts": {
    "build": "pnpm run build:css",
    "build:css": "tsx scripts/build-css.ts",
    "lint": "stylelint \"src/**/*.css\"",
    "test": "vitest run"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "cssnano": "^7.0.5",
    "postcss": "^8.4.40",
    "postcss-cli": "^11.0.0",
    "stylelint": "^16.8.2",
    "tsx": "^4.16.2",
    "vitest": "^2.0.5"
  }
}
